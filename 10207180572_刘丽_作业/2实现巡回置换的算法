#include<iostream.h> 
void main() 
{ 
int p[7]={4,7,3,1,2,5,6}; 
int x,k; 
k=0; 
while(k<=6) 
{ 
x=k+1; 
do 
{ 
cout<<x; 
x=p[x-1]; 
}while(x != k+1); 
cout<<endl; 
k++; 
} 
} 

#include<stdio.h>
main()
{
   int x,fd[2];
   char buf[30],s[30];
   pipe(fd);
   while((x=fork())==-1;
   if(x==0)
   {
        sprintf(buf,"This is an example\n");
        wtite(fd[1],buf,30);
        exit(0);
    }
    else
    {
        wait(0);
        read(fd[0],s,30);
        printf("ï¹ªs",s);
    }
}
 
